    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Prepare BCSS from TIGER",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg_fewshot/preprocessing/bcss/prepare.py",
            "args": [
                "--raw-data-dir",
                "/mnt/nas7/data/Personal/Valentin/aws_downloads/tiger-training/wsirois/roi-level-annotations/tissue-bcss",
                "--output-dir",
                "/home/valentin/data/fss/bcss",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Build FSS index",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg_fewshot/preprocessing/build_indexes.py",
            "args": [
                "--root-dir",
                "/home/valentin/data/fss",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Build FSS tile index",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg_fewshot/preprocessing/build_index_tile.py",
            "args": [
                "--root-dir",
                "/home/valentin/data/fss",
                "--output-dir",
                "${workspaceFolder}/data/index",
                "--tile-size",
                "672",
                "--stride",
                "448",
                "--concat-global",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Prepare IGNITE",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg_fewshot/preprocessing/ignite/prepare.py",
            "args": [
                "--raw-data-dir",
                "/mnt/nas7/data/Personal/Valentin/IGNITE",
                "--output-dir",
                "/home/valentin/data/fss/ignite",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Prepare ANORAK",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg_fewshot/preprocessing/anorak/prepare.py",
            "args": [
                "--raw-data-dir",
                "/mnt/nas7/data/Personal/Valentin/ANORAK_not_resized",
                "--output-dir",
                "/home/valentin/data/fss/anorak",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python: main.py fit FewShotSegmenter",
            "type": "debugpy",
            "request": "launch",
            "module": "pathseg_fewshot",
            "args": [
                "fit",
                "-c",
                "/home/valentin/workspaces/pathseg-fewshot/configs/fss_meta_semantic.yaml",
                "--runs_dir",
                "runs/",
                "--no_compile",
                "--trainer.logger=false",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "CUDA_VISIBLE_DEVICES": "0",
                "HYDRA_FULL_ERROR": "1"
            },
            "justMyCode": false
        },
    ]
}